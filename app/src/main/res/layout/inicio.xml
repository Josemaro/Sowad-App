<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#6CDD4D"
    android:orientation="vertical"
    android:padding="95dp"
    app:layout_behavior="com.google.android.material.appbar.AppBarLayout">


    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="120dp"
        android:layout_height="150dp"
        app:srcCompat="@mipmap/ic_deliv"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="255dp" />

    <Button
        android:id="@+id/btnDel"
        android:layout_width="122dp"
        android:layout_height="33dp"
        android:background="#6CDD4D"
        android:fontFamily="@font/bangers"
        android:text="Deliveries"
        android:textColor="#000000"
        tools:layout_editor_absoluteX="52dp"
        tools:layout_editor_absoluteY="407dp" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="120dp"
        android:layout_height="150dp"
        app:srcCompat="@mipmap/ic_prod"
        tools:layout_editor_absoluteX="207dp"
        tools:layout_editor_absoluteY="255dp" />

    <Button
        android:id="@+id/btnProd"
        android:layout_width="122dp"
        android:layout_height="33dp"
        android:background="#6CDD4D"
        android:fontFamily="@font/bangers"
        android:text="Productos"
        android:textColor="#000000"
        tools:layout_editor_absoluteX="52dp"
        tools:layout_editor_absoluteY="407dp" />


</LinearLayout>